# ##############################################################################
# dunst setup configurations
# ##############################################################################

[global]
  geometry = "300x5-8+40"
  frame_width = 1
  frame_color = "#dc322f"
  separator_color = "#dc322f"
  padding = 8
  horizontal_padding = 8
  font = Fira Code 10
  markup = full
  format = "<b>%s</b>\n%b"
  word_wrap = true
  alignment = left
  browser = /usr/bin/firefox-dev -new-tab
[urgency_low]
  background = "#002b36"
  foreground = "#eee8d5"
  timeout = 10
[urgency_normal]
  background = "#002b36"
  foreground = "#eee8d5"
  timeout = 10
[urgency_critical]
  background = "#002b36"
  foreground = "#eee8d5"
  frame_color = "#002b36"
  timeout = 10
